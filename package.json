{
  "name": "osm-verkehrswende-org",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "npm run dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "type-check": "astro check && tsc --skipLibCheck",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|astro|mdx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|astro|mdx)\" --write",
    "check": "npm run type-check && npm run format && npm run lint",
    "cqi:createPmtiles": "npm run cqi:createPmtiles:pmTiles && npm run cqi:createPmtiles:upload",
    "cqi:createPmtiles:pmTiles": "node ./scripts/cqi/createPmtiles/createPmtiles.js",
    "cqi:createPmtiles:upload": "node ./scripts/cqi/createPmtiles/upload.js",
    "cqi:createMbtiles": "node ./scripts/cqi/createMbtiles/createMbtiles.js",
    "cqi:routing": "npm run cqi:routing:geojson && npm run cqi:routing:graph && npm run cqi:routing:copy",
    "cqi:routing:geojson": "chmod +x ./scripts/cqi/routing/createGeojson.sh && ./scripts/cqi/routing/createGeojson.sh",
    "cqi:routing:graph": "chmod +x ./scripts/cqi/routing/createGraph.sh && ./scripts/cqi/routing/createGraph.sh",
    "cqi:routing:copy": "chmod +x ./scripts/cqi/routing/copyPublicFiles.sh && ./scripts/cqi/routing/copyPublicFiles.sh",
    "updatePackages:major": "npx taze major --write && npm install",
    "updatePackages:minor": "npx taze minor --write && npm install",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.10",
    "@astrojs/mdx": "^2.2.3",
    "@astrojs/react": "^3.1.1",
    "@astrojs/rss": "^4.0.5",
    "@astrojs/sitemap": "^3.1.2",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/ts-plugin": "^1.6.1",
    "@aws-sdk/client-s3": "^3.540.0",
    "@headlessui/react": "^1.7.18",
    "@nanostores/react": "^0.7.2",
    "@nanostores/router": "^0.14.1",
    "@turf/turf": "^7.0.0-alpha.114",
    "astro": "^4.5.15",
    "maplibre-gl": "^4.1.2",
    "nanostores": "^0.10.2",
    "pmtiles": "^3.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "7.2.0-beta.1",
    "remark-toc": "^9.0.0",
    "route-snapper": "^0.4.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.12",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.13"
  }
}
