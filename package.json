{
  "name": "osm-verkehrswende-org",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "npm run dev",
    "build": "astro build",
    "build:local": "ASTRO_OUTPUT_MODE=static astro build",
    "preview": "astro preview",
    "astro": "astro",
    "extract-project-keys": "bun run keystatic/scripts/extractProjectKeys.ts && prettier 'keystatic/extractedProjectKeys.ts' --write",
    "generate-types": "astro sync --verbose && cat ./.astro/astro/content.d.ts",
    "type-check": "astro check && tsc --skipLibCheck --noEmit",
    "lint": "eslint",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|astro|mdx)\" --write",
    "check": "npm run type-check && npm run format && npm run lint",
    "cqi:createPmtiles": "npm run cqi:createPmtiles:pmTiles && npm run cqi:createPmtiles:upload",
    "cqi:createPmtiles:pmTiles": "node ./scripts/cqi/createPmtiles/createPmtiles.js",
    "cqi:createPmtiles:upload": "node ./scripts/cqi/createPmtiles/upload.js",
    "cqi:createMbtiles": "node ./scripts/cqi/createMbtiles/createMbtiles.js",
    "cqi:routing": "npm run cqi:routing:geojson && npm run cqi:routing:graph && npm run cqi:routing:copy",
    "cqi:routing:geojson": "chmod +x ./scripts/cqi/routing/createGeojson.sh && ./scripts/cqi/routing/createGeojson.sh",
    "cqi:routing:graph": "chmod +x ./scripts/cqi/routing/createGraph.sh && ./scripts/cqi/routing/createGraph.sh",
    "cqi:routing:copy": "chmod +x ./scripts/cqi/routing/copyPublicFiles.sh && ./scripts/cqi/routing/copyPublicFiles.sh",
    "updatePackages:major": "npx taze major --includeLocked --write --exclude eslint && npm run updatePackages:minor && npm install",
    "updatePackages:minor": "npx taze minor --includeLocked --write && npm install",
    "bleach": "rm -rf node_modules/.astro && rm -rf node_modules/.bin && rm -rf node_modules/.vite && rm -rf .astro/ && rm -rf .netlify/ && rm -rf dist/ && npm install && npm run bleach:info",
    "bleach:info": "echo \"\u001b[43m\u001b[30m(!) Also delete the browser cache, see https://github.com/vitejs/vite/discussions/17738#discussioncomment-10158746\u001b[0m\"",
    "bleach:all": "rm -rf node_modules/ && npm run bleach",
    "bleach:full": "rm package-lock.json && npm run bleach:all",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.3",
    "@astrojs/mdx": "^3.1.5",
    "@astrojs/react": "^3.6.2",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/ts-plugin": "^1.10.1",
    "@aws-sdk/client-s3": "^3.645.0",
    "@headlessui/react": "^2.1.5",
    "@heroicons/react": "^2.1.5",
    "@keystatic/astro": "^5.0.0",
    "@keystatic/core": "^0.5.32",
    "@nanostores/react": "^0.7.3",
    "@nanostores/router": "^0.15.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@turf/turf": "^7.1.0",
    "astro": "^4.15.3",
    "astro-heroicons": "^2.1.5-1",
    "maplibre-gl": "^4.6.0",
    "nanostores": "^0.11.3",
    "pmtiles": "^3.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-map-gl": "7.2.0-beta.1",
    "remark-toc": "^9.0.0",
    "route-snapper": "^0.4.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^1.2.3",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "husky": "^9.1.5",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "vite-plugin-watch-and-run": "^1.7.1"
  }
}
